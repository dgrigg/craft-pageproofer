{
  "name": "dgrigg/craft-3-boilerplate",
  "description": "Craft 3 boilerplate setup, includes basic js/css/webpack setup",
  "version": "1.0.7",
  "authors": [
    {
      "name": "Derrick Grigg",
      "email": "derrick@dgrigg.com"
    }
  ],
  "support": {
    "docs": "https://github.com/dgrigg/craft-3-boilerplate/blob/master/README.md",
    "issues": "https://github.com/dgrigg/craft-3-boilerplate/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "~/www/craft-plugins/*",
      "options": {
        "symlink" : true
      }
    }
  ],
  "require": {
    "craftcms/cms": "3.4.6.1",
    "vlucas/phpdotenv": "^2.4.0",
    "verbb/super-table": "^2.1.16",
    "verbb/navigation": "^1.1.11",
    "craftcms/redactor": "2.3.3.2",
    "sebastianlenz/linkfield": "1.0.20",
    "nystudio107/craft-seomatic": "^3.1.46",
    "spicyweb/craft-neo": "^2.2.8",
    "dgrigg/craft-migration-assistant": "3.1.6",
    "studioespresso/craftnet-cp": "^1.3",
    "craftcms/commerce": "^3.0",
    "studioespresso/craft-seeder": "^3.2",
    "barrelstrength/sprout-fields": "^3.8",
    "barrelstrength/sprout-reports": "^1.5",
    "dgrigg/pageproofer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
